{
    "files.associations": {
        "**/_includes/**/*.html": "jekyll",
        "**/_layouts/**/*.html": "jekyll"
    },
    "emmet.includeLanguages": {
        "jekyll": "html"
    },
    "files.exclude": {
        ".venv": true,
        "**/__pycache__": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "python.analysis.typeCheckingMode": "strict",
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true
}